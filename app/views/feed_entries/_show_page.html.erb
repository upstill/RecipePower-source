<div class="feed-entry" id="<%= decorator.dom_id %>">
  <h3><a href='<%= decorator.object.url %>' class="tablink"><%= decorator.title %></a><%= collectible_buttons_panel decorator, :button_size => "xs" %></h3>

  <p>
    <%= decorator.object.published_at ? %Q{<p>Published on: #{decorator.object.published_at.strftime("%B %d, %Y")}</p>}.html_safe : "" %>
  </p>

  <p>
    <%= (decorator.object.summary || "").html_safe %>
  </p>
  <hr>
</div>
