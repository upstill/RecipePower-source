<div class="feed-entry page" id="<%= decorator.dom_id %>">
  <div class="headline">
    <a href='<%= decorator.object.url %>' class="tablink"><%= decorator.title %></a>
    <%# collectible_buttons_panel decorator, :button_size => "xs" %>
    <%= collectible_collect_icon decorator %>
    <%= collectible_list_menu decorator, current_user, { } %>
    <%= collectible_tag_button decorator, { } %>
    <%= collectible_share_button decorator, { } %>
  </div>

  <div class="published">
    <%= decorator.object.published_at ? %Q{<p>Published on: #{decorator.object.published_at.strftime("%B %d, %Y")}</p>}.html_safe : "" %>
  </div>

  <div class="summary">
    <%= (decorator.object.summary || "").html_safe %>
  </div>
  <hr>
</div>
