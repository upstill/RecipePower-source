<%# render "show", decorator: decorator %>
<%= present decorator, current_user_or_guest do |presenter| %>
    <div class="card-item row card-large <%= presenter.card_kind %> <%= decorator.dom_id %>">
      <div class="col-md-4">
        <%= presenter.card_avatar %>
      </div>
      <div class="col-md-8">
        <% ch = presenter.card_header %>
        <%= presenter.card_header %>
        <%# presenter.card_subhead %>
        <div class="row">
          <div class="col-md-12">
              <% presenter.card_aspects.each do |aspect | %>
                  <%= presenter.card_aspect_rendered aspect %>
              <% end %>
          </div>
        </div>
      </div>
    </div>
<% end %>
