<% prompt = "welcomes you! Already a member? " %>
<%# prompt << link_to_redirect("Sign up!", hash_to_modal( new_user_registration_url, home_url )) %>
<% prompt << link_to_modal("Sign in!", new_user_session_path(@user, target: "injector")) %>
<%= injector_dialog :new_registration, prompt.html_safe, :noflash => true, dismiss_button: false do %>
    <hr>
    <%= render 'authentications/possible', prompt: "You can click one of these to sign up...&nbsp;&nbsp", intention: "signup" %>
    <div class="or"><h4>OR</h4></div>
    <%= render "registrations/form" %>
<% end %>