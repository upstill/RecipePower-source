<%= form_for(resource, :as => resource_name, :url => session_path(resource_name, response_service.redirect_params), :html => {'data-type' => 'json'}) do |f| %>
    <%# devise_error_messages! %>
    <%= form_errors_helper f, resource %>
    <div style="width:50%; margin: 10px;">
      <div class="signin_credentials" style="text-align:right; line-height: 200%;">
        <p><%= f.label :username, "Username (Handle)".html_safe, class: "signin_element" %>
        <%= f.text_field :username, class: "signin_element", placeholder: "CoolBeans", style: "width: 250px" %></p>
        <p><%= f.label :fullname, "Full Name".html_safe, class: "signin_element" %>
        <%= f.text_field :fullname, class: "signin_element", placeholder: "Joe Cool", style: "width: 250px" %></p>
        <p><%= f.label :email, "Email".html_safe, class: "signin_element" %>
        <%= f.text_field :email, class: "signin_element", placeholder: "peerless@nonpareil.net", style: "width: 250px" %></p>
        <p><%= f.label :password, class: "signin_element" %>
        <%= f.password_field :password, class: "signin_element", style: "width: 250px" %></p>
      </div>
      <%= f.submit "Sign me up!", class: "btn btn-success dialog-submit-button" %>
    </div>
<% end %>
