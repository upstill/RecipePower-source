<%= modal_dialog :show_feed, "Feed #{@feed.title}" do %>
    <%= render "collectible/show_card", fallback_pic: (fallback_pic if defined?(fallback_pic)), buttons_list: buttons_list %>
    <% permitted_to?(:approve, :feeds) do %>
        <p>
          Feed <%= feed_approval @feed %>
        </p>
    <% end %>
<% end %>