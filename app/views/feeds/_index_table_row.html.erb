<% item ||= @feed %>
<tr id="<%= dom_id item %>">
  <td>
    <%= link_to_submit item.title.truncate(80), feed_path(item), :mode => :partial %><br>
    <%= item.description %><br>
  </td>
  <td><%= taggable_list item %></td>
  <td><%= item.feedtypename unless item.feedtypename == "Misc" %></td>
  <td><%= link_to_submit item.site.name, site_path(item.site), mode: :partial %></td>
  <td><%= feed_status_summary item %></td>
  <% if response_service.admin_view? %>
      <td><%= feed_approval @feed %></td>
  <% end %>
  <td>
  <%= collectible_table_buttons item, false do %>
    <%= feed_buttons @feed, button_size: "sm" %>
  <% end %>
  </td>
</tr>
