<!-- View for listing entities in a table -->
<table class="table table-striped ">
  <thead>
  <tr>
    <%= @filtered_presenter.table_headers.collect { |ttl| "<th>#{ttl}</th>" unless ttl.blank? }.compact.join("\n").html_safe %>
  </tr>
  </thead>
  <tbody class="stream-items-parent <%= @filtered_presenter.stream_id %>">
    <%= render @filtered_presenter.tail_partial %>
  </tbody>
</table>
