<!-- Partial for listing entities in a masonry container -->
<% viewparams = @viewparams || (@filtered_presenter && @filtered_presenter.viewparams) unless defined?(viewparams) && viewparams %>
  <div class="results <%= viewparams.result_type.extensions_to_classes %> js-masonry stream-items-parent <%= viewparams.stream_id %> "
       id="container"
       onload="RP.masonry.onload(event)"
       data-masonry-options='{ }'>
    <%= filtered_presenter_tail viewparams  %>
  </div>
