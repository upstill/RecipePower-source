<%= querify_block url, class: "associated-card card-item #{type.extensions_to_classes}", autoload: true do %>
    <% if defined?(title) %>
        <div class="associated-title <%= type.extensions_to_classes %>"><%= title %></div>
    <% end %>
    <div class="results-content">
      <div class="results-box">
        <%= panel_results_placeholder type %>
      </div>
    </div>
<% end %>
