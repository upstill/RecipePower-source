<% if next_path = @sp.next_path %>
  <%= link_to "Click to load", "#",
              onclick: 'RP.stream.go(event);',
              onload: 'RP.stream.onload(event);',
              class: "stream-trigger",
              :"data-path" => next_path,
              :"data-container_selector" => response_service.container_selector %>
<% else %>
  No mo' stuff.
<% end %>
