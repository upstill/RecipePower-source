<!-- View for listing entities in a masonry container -->
<div class="row">
  <div class="js-masonry stream-items-parent col-md-12"
       id="container"
       onload="RP.masonry.onload(event)"
       data-masonry-options='{ }'>
    <% if @filtered_presenter.dump? %>
        <% while (item = @filtered_presenter.next_item) do %>
            <%= render_item item %>
        <% end %>
    <% end %>
  </div>
</div>
