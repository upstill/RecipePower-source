<%# Render a Rcpref by rendering its entity %>
<% cache [decorator.entity, decorator.comment, :slider] do %>
    <% viewparams = @viewparams || (@filtered_presenter && @filtered_presenter.viewparams) unless defined?(viewparams) && viewparams %>
    <%# Template Dependency: collectible/show_slider %>
    <%= render_item_unwrapped decorator.object.entity.decorate, :slider, viewparams: viewparams, comment: decorator.object.comment %>
<% end %>