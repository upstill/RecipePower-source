<% prompt ||= "You can sign in using one of these fine services:" %>
<div class="auth_poss <%= response_service.area_class %>" style="overflow: visible">
  <h4><%= prompt.html_safe %></strong></h4>
  <div class="row">
    <div class="col-md-2 auth"><%= auth_possible "Facebook" %></div>
    <div class="col-md-2 auth"><%= auth_possible "Google", :svc_lower => "google_oauth2" %></div>
    <div class="col-md-2 auth"><%= auth_possible "Twitter" %></div>
    <div class="col-md-2 auth"><%= auth_possible "Aol" %></div>
    <div class="col-md-2 auth"><%= auth_possible "OpenID", :svc_lower => "open_id" %></div>
    <div class="col-md-2 auth"><%= auth_possible "Yahoo" %></div>
  </div>
</div>
