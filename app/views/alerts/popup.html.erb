<% if response_service.injector? %>
	<%= injector_dialog :alert, "has heard your command, and obeys.", dismiss_button: "Ack" do %>
		<%= render "alerts/injector_popup" %>
	<% end %>
<% else %>
	<%= modal_dialog "popup new-style purple", "HELP IS ON ITS WAY" do %>
		<%= render "alerts/popup" %>
	<% end %>
<% end %>
