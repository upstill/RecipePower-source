<p id="notice"><%= notice %></p>

<h1>Listing Tagsets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tagsets.each do |tagset| %>
      <tr>
        <td><%= link_to 'Show', tagset %></td>
        <td><%= link_to 'Edit', edit_tagset_path(tagset) %></td>
        <td><%= link_to 'Destroy', tagset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tagset', new_tagset_path %>
